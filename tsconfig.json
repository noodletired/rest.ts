{
    "compilerOptions": {
        "baseUrl": "packages",
        "strict": true,
        "importHelpers": false,
        "inlineSourceMap": true,
        "declaration": true,
        "target": "es5",
        "module": "commonjs",
        "removeComments": false,
        "plugins": [
            { "name": "tslint-language-service" }
        ],
        "lib": [
            "es2015"
        ],
        "paths": {
            "rest-ts-axios": ["rest-ts-axios/src"],
            "rest-ts-core": ["rest-ts-core/src"],
            "rest-ts-express": ["rest-ts-express/src"],
        }
    }
}
